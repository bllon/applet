/**label.wxss**/
.container {
    height: 100%;
    display: flex;
    flex-direction: row;
    align-items: initial;
    justify-content: space-between;
    padding: 20px 0;
    box-sizing: border-box;
  }
  .nav {
    width: 100%;
    height: auto;
    padding: 0 10px;
  }
  .box {
    width: auto;
    height: 30px;
    line-height:30px;
    text-align:center;
    border-radius: 10px;
    letter-spacing:2px;
    float: left;
    padding:0 5px;
    margin-right: 5px;
    margin-top:10px;
    color: #f14f03;
    background: #f1f6f7;
    box-shadow: 0px 0px 2px #aaa;
  }